
<%= link_to("<< روجوع", {:action => 'list'}, :class => 'back-link') %>

<div>
  
  <%= form_for(:community, :url => {:action => 'update', :id => @community.id}) do |t| %>
  
<%= error_messages_for(@community) %>

    <table>
      <tr>
        <p class= "text-center">
        <th>عنوان</th>
        <td><%= t.text_field(:title) %></td>
      </tr>
      <tr>
        <th>معلومات عن المجتمع </th>
        <td><%= t.text_field(:social_profile_info) %></td>
      </tr>
      <tr>
        <th>الارقام</th>
        <td><%= t.text_field(:contact_info) %></td>
      </tr>
    </table>
    <p class="text-center">
<button class="btn btn-success" type="submit">تحديث</button>
    </p>
  <% end %>
</div>
