
<%= link_to("<< Back to List", {:action => 'list'}, :class => 'back-link') %>

<div>
  <h2>Create community</h2>

  <%= form_for(:community, :url => {:action => 'createCommunity'}) do |t| %>

  <%= error_messages_for(@community) %>


    <table>
      <tr>

        <th>Title</th>
        <td><%= t.text_field(:title) %></td>
      </tr>
      <tr>
        <th>Social_profile_info</th>
        <td><%= t.text_field(:social_profile_info) %></td>
      </tr>
      <tr>
        <th>Contact info</th>
        <td><%= t.text_field(:contact_info) %></td>
      </tr>
    </table>
<button class="btn btn-success" type="submit">Button</button>

    <div>
      <%= submit_tag("Create community") %>
    </div>
    
  <% end %>
</div>
