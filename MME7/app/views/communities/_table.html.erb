

<table class="listing" summary="community list">

    <tr class="header">
      <th>Id</th>
      <th></th>
      <th>Title</th>
      <th></th>
      <th>Social profile info</th>
      <th></th>
      <th>Contact info</th>
      <th></th>
      <th>Actions</th>
    </tr>
    <% @communities.each do |community| %>
    <tr>
    
        <td><%= community.id %></td>
        <td></td>
        <td><%= community.title %></td>
        <td></td>
        <td><%= community.social_profile_info %></td>
        <td></td>
        <td><%= community. contact_info %></td>
        <td></td>
        <td> 
        <%if community.is_dismissed != true %>
            <%= link_to("Edit", {:action => 'edit', :id => community.id}, :class => 'action edit') %>
            <%= link_to("Delete", {:action => 'destroy', :id => community.id}, :confirm => "Are you sure you want to dismiss this community", :class => 'action delete') %>
              <% end %> 
        </td>
     
    </tr>
    <% end %>
    
</table>