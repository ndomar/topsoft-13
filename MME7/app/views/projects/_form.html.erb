<table class="table table-striped" dir="rtl" >
 	 <tr>
 		<th><%= "الاسم " %></th>
 		<td><%= f.text_field(:name) %></td>
 	 </tr>
 	
 	 <tr>
 		<th><%=  "تاريخ البداية" %> </th>
 		<td><%= f.datetime_select(:start_date, :order => [:day, :month, :year]) %></td>
 	 </tr>

 	 <tr>
 		<th><%=  "تاريخ النهاىة" %> </th>
 		<td><%= f.datetime_select(:end_date, :order => [:day, :month, :year]) %></td>
 	 </tr>

 	 <tr>
 		<th><%=  "التفاصيل " %></th>
 		<td><%= f.text_field(:description) %></td>
 	 </tr>

	<p>
 	 	<%= f.label :user_tokens, "الأعضاء" %><br />
        <%= f.text_field :user_tokens, "data-pre" => @project.users.map(&:attributes).to_json %>
 	 </p>

</table>

  