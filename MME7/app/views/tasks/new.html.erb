<div >
	<h2>Create Task</h2>
	<%= form_for(:task, :url => {:action => 'create', :id =>params[:id], :project_id => params[:project_id]}) do |f| %>

	<table>
		

		<tr>
			<th>Task description</th>
			<td><%= f.text_field (:description) %></td>
		</tr>


		<tr>
  <%= f.label :user_tokens, "Assign members " %><br />
  <%= f.text_field :user_tokens, "data-pre" => @task.users.map(&:attributes).to_json %>
</tr>

	</table>
	

	<div >  
		<%= submit_tag("Create task") %>
	</div>

	<% end %>

</div>