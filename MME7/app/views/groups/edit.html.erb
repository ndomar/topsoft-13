
<%= link_to("<< Back to List", {:action => 'list'}, :class => 'back-link') %>

<div>
  <h2>Edit Group</h2>

  <%= form_for(:group, :url => {:action => 'update', :id => @group.object_id}) do |t| %>
  
    <table>
      <tr>
        <th>Name</th>
        <td><%= t.text_field(:group_name) %></td>
      </tr>
      <tr>
        <th>Description</th>
        <td><%= t.text_field(:description) %></td>
      </tr>
      <tr>
        <th>Levels</th>
     <td><%= t.text_field(:Levels) %></td>
      </tr>
    </table>
    
    <div>
      <%= submit_tag("update community") %>
    </div>
    
  <% end %>
</div>