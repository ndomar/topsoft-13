
<div class="flash">
  <%= flash[:notice] %>
  <%= flash[:error] %>
</div>

<h1>Groups</h1>   
<div>
<%= link_to("Add New Group", {:action => 'new'}, :class => 'action new') %>
</div>

<%= form_tag('groups/', :method => 'get',:id => "search",:remote => true) do %>  
